@techreport{berta_cheops_2016,
	type = {{TESS} {Internal} {Report}},
	title = {Projects for a {TESS} and {CHEOPS} {Collaboration}},
	abstract = {The TESS and CHEOPS missions have complementary capabilities.
	TESS uses 10cm telescopes with wide fields-of-view to survey most
	of the sky for transiting planets. CHEOPS uses a 30cm telescope with
	a narrow field-of-view to provide precise photometry of individual
	targets. TESS will provide temporal coverage that increases from
	27.4 days at low ecliptic latitudes up to one year at the ecliptic poles.
	CHEOPS’ visibility is maximized closest to the ecliptic, and drops to
	zero at the poles. This memo outlines and quantifies how CHEOPS
	observations of TESS planet candidates can advance the science cases
	of both missions.},
	institution = {MIT},
	author = {Berta-Thompson, Zach and Ribas, Ignasi and Seager, Sara and Bouma, Luke},
	month = jun,
	year = {2016}
}


